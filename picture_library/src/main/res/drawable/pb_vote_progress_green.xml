<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 进度条背景色 -->
    <item android:id="@android:id/background">
        <shape>
            <corners
                android:bottomLeftRadius="4dp"
                android:bottomRightRadius="4dp" />
            <solid android:color="@color/color_F9F" />
            <size android:height="12dp" />
            <stroke
                android:width="0.5dp"
                android:color="@color/color_EFE" />
        </shape>
    </item>

    <!-- 第一进度条 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:bottomLeftRadius="4dp"
                    android:bottomRightRadius="4dp" />
                <gradient

                    android:angle="45"
                    android:centerColor="@color/color_bae"
                    android:endColor="@color/color_bae"
                    android:startColor="@color/color_bae" />
                <size android:height="12dp" />
                <stroke
                    android:width="0.5dp"
                    android:color="@android:color/transparent" />
            </shape>
        </clip>
    </item>
</layer-list>
